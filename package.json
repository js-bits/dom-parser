{
  "name": "@js-bits/dom-parser",
  "version": "0.0.3",
  "description": "Cross-environment (nodejs/web) DOM parser for XML and HTML",
  "keywords": [
    "javascript",
    "xml",
    "html",
    "dom",
    "parser",
    "web",
    "node",
    "browser"
  ],
  "type": "module",
  "module": "./index.js",
  "exports": "./index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch",
    "lint": "eslint '**/*.{js,jsx}'"
  },
  "browser": {
    "jsdom": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/dom-parser"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "jsdom": "^16.6.0"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^0.1.2",
    "@js-bits/log-in-color": "^0.0.2",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom"
      }
    ]
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  }
}
